{
  "name": "hrx-labs",
  "version": "1.0.0",
  "description": "hrx lab module",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=8.3.2"
  },
  "scripts": {
    "postinstall": "npx is-ci || npx husky install",
    "test:ci": "cross-env NODE_ENV=test  mocha test --reporter mocha-junit-reporter --timeout 30000 --recursive --exit",
    "test": "cross-env NODE_ENV=test mocha test/ --timeout 30000  --recursive --exit",
    "lint": "eslint src/**/*.js test/ --fix",
    "lint:fix": "eslint --fix",
    "format": "npx prettier --write src/**/*.js test/**/*.js",
    "test:coverage": "nyc --reporter=lcov --reporter=html --reporter=cobertura npm run test:ci",
    "start:debug": "node --inspect src/index",
    "start": "node --inspect src/index",
    "start:uat": "node --inspect src/index.js",
    "start:dev": "env-cmd -f .env.dev node --inspect src/index.js",
    "start:local": "env-cmd -f .env nodemon src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "9.1.1",
    "@hapi/joi-date": "2.0.1",
    "applicationinsights": "1.8.9",
    "axios": "^0.24.0",
    "bluebird": "3.5.1",
    "compression": "~1.7.4",
    "config": "3.3.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "env-cmd": "10.1.0",
    "express": "4.17.1",
    "express-mongo-sanitize": "2.0.2",
    "form-data": "4.0.0",
    "helmet": "4.4.1",
    "http-status-codes": "1.4.0",
    "into-stream": "6.0.0",
    "joi": "17.4.0",
    "joi-objectid": "3.0.1",
    "joi-phone-number": "5.0.1",
    "js2xmlparser": "4.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "2.1.28",
    "moment": "2.27.0",
    "mongoose": "5.9.19",
    "mongoose-aggregate-paginate-v2": "1.0.5",
    "mongoose-paginate-v2": "1.3.14",
    "morgan": "1.10.0",
    "nodemailer": "^6.7.2",
    "promise-ftp": "1.3.5",
    "redis": "3.1.0",
    "redis-mock": "0.56.3",
    "sinon": "10.0.1",
    "swagger-ui-express": "^4.3.0",
    "uuid": "8.3.2",
    "validator": "^13.7.0",
    "winston": "3.3.1",
    "xlsx": "^0.17.5",
    "xss-clean": "0.1.1",
    "yamljs": "0.3.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test"
    ]
  },
  "devDependencies": {
    "chai": "4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "mocha": "7.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mongodb-memory-server": "6.9.3",
    "nock": "13.0.7",
    "node-mocks-http": "1.10.1",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "supertest": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
